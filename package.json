{
  "name": "template-nextjs",
  "license": "UNLICENSED",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "create:fc": "hygen new fc",
    "create:pc": "hygen new pc",
    "dev": "next dev",
    "build": "next build",
    "generate": "yarn build && next-export-optimize-images",
    "start": "next start",
    "fix": "prettier --write './**/*.{js,jsx,ts,tsx}'",
    "lint": "next lint . --cache",
    "lint:fix": "next lint . --cache --fix",
    "lint-staged": "lint-staged",
    "tsc": "tsc",
    "prepare": "husky install",
    "postbuild": "next-sitemap --config sitemap.config.js"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --wirte"
    ]
  },
  "dependencies": {
    "clsx": "2.0.0",
    "dayjs": "1.11.10",
    "focus-visible": "5.2.0",
    "framer-motion": "10.16.4",
    "microcms-js-sdk": "2.7.0",
    "next": "13.5.6",
    "next-head-seo": "0.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "scroll-behavior-polyfill": "2.0.13",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.6",
    "@types/node": "18.18.6",
    "@types/react": "18.2.31",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "lint-staged": "15.0.2",
    "next-export-optimize-images": "3.2.0",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.31",
    "postcss-import": "15.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "typescript": "5.2.2"
  }
